#include <asf.h>
#include <stdio.h>
#include <avr/io.h>
#include <avr/interrupt.h>
#include "uart.h"
#include "util/delay.h"

ISR(TIMER1_CAPT_vect) {
	TCCR1B ^= 0x40;
	PORTB ^= 0x20;
	putchar('s');
}

int main (void)
{
	uart_init();
	DDRB = 0x20;	//enable pin 13 write
	TCCR1B = 0x01;	//setting the clock on
	TIMSK1 = 0x20;	//allow interrupts
	sei();
	while(1);
}
